using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class functions : MonoBehaviour
{
    
    // Start is called before the first frame update
    void Start()
    {
        
        Debug.Log(sum(3,4));
    }

    // Update is called once per frame
    void Update()
    {
        
    }

    public int sum(int a,int b){
        int result = a + b;
        return result;
    }
}
